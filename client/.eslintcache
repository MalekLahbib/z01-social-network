nconst StyledNav = styled.nav `\n    margin: 1px;\n    height: 50px;\n    padding: 10px;\n    display: flex;\n    align-items: center;\n    justify-content: space-between;\n    border: solid 1px;\n    border-radius: 10px;\n    background: ${props => (props.theme === 'light' ? colors.backgroundLight : colors.backgroundDark)};\n`\n\nconst StyledGroupIcn = styled.div `\n    display: flex;\n    align-items: center;\n    justify-content: space-between;\n`\n\nconst Navbar = () => {\n    const { theme } = useContext(ThemeContext)\n    const { authPseudo, photoProfile, isAuthenticated, handleLogout } = useContext(AuthContext)\n\n    return (\n        <StyledNav theme={theme}>\n            <StyledGroupIcn>\n                <Link to=\"/\">\n                    <Icone \n                        alt=\"Page d'accueil\" \n                        image={IcnHome}\n                        disabled={false}\n                    />\n                </Link>\n                <Link to=\"/users\">\n                    <Icone \n                        alt=\"Communauté\" \n                        image={IcnUsers}\n                        disabled={false}\n                    />\n                </Link>\n                <Link to=\"/groups\">\n                    <Icone \n                        alt=\"Groupes de discution\" \n                        image={IcnGroupe}\n                        disabled={false}\n                    />\n                </Link>\n                <Link to=\"/tchat\">\n                    <Icone \n                        alt=\"Tchat\" \n                        image={IcnTchat}\n                        disabled={false}\n                    />\n                </Link>\n            </StyledGroupIcn>\n\n            <StyledGroupIcn>\n                {isAuthenticated ? (\n                    <>\n                        <Link to=\"/\">\n                            <Button text=\"Se déconnecter\" onClick={handleLogout} disabled={false} />\n                        </Link>\n                        <Link to={`/user/${authPseudo}`}>\n                            <Icone \n                                alt=\"Mon profile\" \n                                disabled={!isAuthenticated} \n                                image={`http://${window.location.hostname}:4000/download/${photoProfile}`}\n                            />\n                        </Link>\n                    </>\n                ) : (\n                    <>\n                        <Link to=\"/login\">\n                            <Button text=\"Se connecter\" disabled={false} />\n                        </Link>\n                        <Link to=\"/register\">\n                            <Button text=\"Créer un compte\" disabled={false} />\n                        </Link>\n                    </>\n                )}\n            </StyledGroupIcn>\n        </StyledNav>\n    )\n}\n\nexport default Navbar","/home/student116/Desktop/Div-01/social-network/client/src/components/pages/LoginForm/LoginForm.jsx",[],"/home/student116/Desktop/Div-01/social-network/client/src/components/Button/Button.jsx",[],"/home/student116/Desktop/Div-01/social-network/client/src/utils/ThemeProvider/ThemeProvider.jsx",[],"/home/student116/Desktop/Div-01/social-network/client/src/utils/AuthProvider/AuthProvider.jsx",[],"/home/student116/Desktop/Div-01/social-network/client/src/components/Tchat/Tchat.jsx",[],"/home/student116/Desktop/Div-01/social-network/client/src/components/pages/Users/Users.jsx",[],"/home/student116/Desktop/Div-01/social-network/client/src/components/pages/Home/Home.jsx",[],"/home/student116/Desktop/Div-01/social-network/client/src/components/Footer/Footer.jsx",[],"/home/student116/Desktop/Div-01/social-network/client/src/components/Error/Error.jsx",[],"/home/student116/Desktop/Div-01/social-network/client/src/components/RadioBouton/RadioBouton.jsx",[],"/home/student116/Desktop/Div-01/social-network/client/src/components/Icone/Icone.jsx",[],"/home/student116/Desktop/Div-01/social-ne